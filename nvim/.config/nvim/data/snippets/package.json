{
  "name": "nvim-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "all",
          "plaintext",
          "markdown",
          "tex",
          "html",
          "global"
        ],
        "path": "./global.json"
      },
      {
        "language": [
          "go"
        ],
        "path": "./go.json"
      },
      {
        "language": [
          "dart"
        ],
        "path": "./dart.json"
      },
      {
        "language": [
          "javascript",
          "typescript",
          "javascriptreact",
          "typescriptreact"
        ],
        "path": "./javascript.json"
      }
    ]
  }
}
